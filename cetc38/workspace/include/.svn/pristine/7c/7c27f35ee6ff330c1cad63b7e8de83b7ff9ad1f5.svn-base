}

//===============================================//rd Core old for be Master wr
	if( !IsWrOrNot && !TransNewOrnot && (ch_id == XMCH0_WR || ch_id == XMCH2_WR)){
	TRANS2CORE(tlm::BEGIN_REQ, DMAC_REQ_CORE_MEM, CH_ID,  (unsigned char*)&recent_rdCoreForDMAWR[CH_ID], SC_ZERO_TIME);
