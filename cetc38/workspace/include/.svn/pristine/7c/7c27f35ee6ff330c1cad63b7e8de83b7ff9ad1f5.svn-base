					}
			}else{//not the first trans
				if(src_rem_x_len_wr[CH_ID] == 0){//check 1D is over or not
					src_rem_y_len_wr[CH_ID] --;
					WriteRemRegs(CH_ID,WR_SRC_Y_REM,src_rem_y_len_wr[CH_ID]);

